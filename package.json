    "karma-jasmine": "^1.0.2",
    "load-grunt-config": "~0.7.2",
    "local-xmlhttprequest": "^1.5.1",
    "mocha": "^3.0.2",
    "mocha-jsdom": "^1.1.0",
    "mockery": "^1.7.0",
    "node-gyp": "^3.4.0",
    "node-jsdom": "^3.1.5",
    "node-sass": "^3.11.2",
    "nodemon": "^1.11.0",
    "phaser-arcade-slopes": "git+https://github.com/richardhealy/phaser-arcade-slopes.git#master",
    "phaser-mock": "git+https://github.com/richardhealy/phaser-mock.git",
    "phaser-shim": "^2.4.8",
    "proxyquire": "^1.7.10",
    "proxyquireify": "^3.2.1",
    "script-loader": "^0.7.0",
    "sinon": "^1.17.6",
    "webpack": "1.12.14",
    "webpack-node-externals": "^1.4.3",
    "winston": "^2.3.0",
    "xmlhttprequest": "^1.8.0"
  },
  "scripts": {
    "test": "jshint . && ./node_modules/.bin/jsxhint . && babel-node --presets es2015 ./node_modules/.bin/_mocha $(find test -name '*.js')",
    "build-phaser": "cd node_modules && cp -fR ./@orange-games/phaser-input/build/phaser-input.js ./../dist/ && cd .. && cd node_modules/phaser-shim && npm install && cd node_modules/phaser && grunt custom --exclude creature,ninja --split true && cd ./../../ && npm run build && gulp build && cd ./../../ && cp -fR ./node_modules/phaser-shim/dist/phaser.js ./dist/phaser.js && cp -fR ./node_modules/phaser-shim/dist/pixi.js ./dist/pixi.js && cp -fR ./node_modules/phaser-shim/node_modules/phaser/build/p2.js ./dist/p2.js",
    "build-init": "npm run build-phaser && gulp build",
    "build": "webpack && npm run build-css",
    "build-no-test": "webpack",
    "build-js": "webpack",
    "build-css": "node-sass --include-path assets/sass assets/sass/main.scss dist/css/main.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "watch-js": "nodemon -e js -x \"npm run test\"",
    "watch-js-build": "nodemon -e js -x \"npm run test && npm run build-js\"",
    "watch": "npm run watch-js && npm run watch-css",
    "prebuild": "npm run test"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "babel-runtime": "^6.11.6"
  },
  "main": "gulpfile.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richardhealy/escape.git"
  },
