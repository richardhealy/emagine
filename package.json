{
  "name": "the-escape",
  "version": "0.1.0",
  "description": "Phaser.js Ship Escape",
  "devDependencies": {
    "@orange-games/phaser-input": "^2.0.2",
    "babel-cli": "^6.14.0",
    "babel-loader": "6.2.7",
    "babel-plugin-namespaces": "^0.2.4",
    "babel-plugin-rewire": "^1.0.0",
